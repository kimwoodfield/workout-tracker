thod: 'GET',\n            headers: {\n              'Content-Type': 'application/json'\n            },\n            credentials: 'include',\n          });\n          const body = await res.json();\n          console.log('body is ...', body);\n          setExercise(body.currentWorkout.RoutineExercises);\n          setRoutine(body.currentWorkout.RoutineName)\n        }\n    \n        doFetch();\n      }, []);\n\n      let currentDate = new Date().toLocaleString();\n\n    return (\n        <Form onSubmit={handleSubmit(onSubmit)}>\n\n        <Group>\n            <p>{routine}</p>\n            <p>{currentDate}</p>\n        </Group>\n\n        <Group>\n            <label>{exercise[0]}</label><br />\n            <input ref={register} type=\"number\" name=\"firstExercise_weight\" placeholder=\"Enter weight in KG...\" /><br />\n            <input ref={register} type=\"number\" name=\"firstExercise_sets\" placeholder=\"Enter sets...\" /><br />\n            <input ref={register} type=\"number\" name=\"firstExercise_reps\" placeholder=\"Enter reps...\" />\n        </Group>\n        <Group>\n            <label>{exercise[1]}</label><br />\n            <input ref={register} type=\"number\" name=\"secondExercise_weight\" placeholder=\"Enter weight in KG...\" /><br />\n            <input ref={register} type=\"number\" name=\"secondExercise_sets\" placeholder=\"Enter sets...\" /><br />\n            <input ref={register} type=\"number\" name=\"secondExercise_reps\" placeholder=\"Enter reps...\" />\n        </Group>\n        <Group>\n            <label>{exercise[2]}</label><br />\n            <input ref={register} type=\"number\" name=\"thirdExercise_weight\" placeholder=\"Enter weight in KG...\" /><br />\n            <input ref={register} type=\"number\" name=\"thirdExercise_sets\" placeholder=\"Enter sets...\" /><br />\n            <input ref={register} type=\"number\" name=\"thirdExercise_reps\" placeholder=\"Enter reps...\" />\n        </Group>\n        <Group>\n            <label>{